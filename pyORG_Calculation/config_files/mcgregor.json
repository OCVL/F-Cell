{
  "version": "0.1",

  "processed": {
    "video_format": "{Modality}_{VidNum}_{Year:.4}{Month:.2}{Day:.2}_strip_ref{}_{}.avi",
    "pipeline_params": {
      "gaus_blur": 0.0,
      "mask_roi": {
        "r": 0,
        "c": 0,
        "width": -1,
        "height": -1
      },
      "modalities": ["Confocal", "CalculatedSplit", "760nm"],
      "correct_torsion": true,
      "custom": {
        "dewarp": "ocvl"
      }
    }
  },

  "pipelined": {
    "image_format": "({LocX},{LocY})/{IDnum}_{Year:4}{Month:2}{Day:2}_{Eye}_({},{})_{FOV_Width}x{FOV_Height}_{VidNum}_{Modality}_extract_reg_avg.tif",
    "video_format": "({LocX},{LocY})/{IDnum}_{Year:4}{Month:2}{Day:2}_{Eye}_({},{})_{FOV_Width}x{FOV_Height}_{VidNum}_{Modality}_extract_reg_cropped_piped.avi",
    "metadata": {
      "type": "text_file",
      "metadata_format": "({LocX},{LocY})/{IDnum}_{Year:4}{Month:2}{Day:2}_{Eye}_({},{})_{FOV_Width}x{FOV_Height}_{VidNum}_{Modality}_extract_reg_cropped_piped.csv",
      "fields_to_load": {
        "framestamps": "FrameStamps"
      }
    },
    "analysis_params": {
      "gaus_blur": 0.0,
      "mask_roi": {
        "r": 0,
        "c": 0,
        "width": -1,
        "height": -1
      }
    }
  }

}