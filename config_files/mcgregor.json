{
  "version": "0.1",

  "preanalysis": {
    "video_format": "reg_{Modality}_{VidNum}_registered_{Year:.4}-{Month:.2}-{Day:.2}{}.{}.avi",
    "recursive_search": false,
    "pipeline_params": {
      "gaus_blur": 1.0,
      "correct_torsion": true,
      "output_folder": "Functional Pipeline",
      "intra_stack_xform": "affine"
    }
  },
  "analysis": {
    "image_format": "piped_{Modality}_{VidNum}_{Year:.4}-{Month:.2}-{Day:.2}_ALL_ACQ_AVG.tif",
    "video_format": "piped_{Modality}_{VidNum}_{Year:.4}-{Month:.2}-{Day:.2}.avi",
    "queryloc_format": "piped_{Modality}_{VidNum}_{Year:.4}-{Month:.2}-{Day:.2}_ALL_ACQ_AVG_coords.csv",
    "metadata": {
      "stimulus_sequence": [501, 1, 491, 1, 491, 1, 491, 1, 491, 1, 491, 1, 491]
    },
    "analysis_params": {
      "summary": {
        "metrics": {
          "measured": "stim-relative",
          "units": "time",
          "prestim": [
            -2,
            0
          ],
          "poststim": [
            0,
            3
          ]
        }
      }
    },
    "display_params": {
      "pause_per_folder": true,
      "pop_summary_overlap": {
        "stimulus": true,
        "relative": true,
        "pooled": true,
        "annotations": true,
        "axes": {
          "xmin": -10,
          "xmax": 10,
          "cmap": "plasma",
          "legend": true
        }
      },
      "saveas": ["png", "svg"]
    }
  }
}